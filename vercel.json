{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "excludeFiles": "**/*"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "ignore": [
        "app",
        "venv",
        "notebook",
        ".vercel",
        "logs",
        "eda.py",
        "__pycache__",
        "setup.py",
        "test.py",
        "artifacts"
    ]
}
